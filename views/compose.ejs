<%- include("partials/header"); %>

<h1>Compose</h1>
<form action="/compose" method="post">
  <div class="form-group">
    <label for="title">Title</label>
    <input class="form-control" id="title" type="text" name="blogtitle">
  </div>
  <div class="form-group">
    <label for="post">Post</label>
    <textarea class="form-control" id="post" name="blogpost" rows="8" cols="80"></textarea>
  </div>
  <button class="btn btn-primary btn" type="submit" name="button">Publish</button>
</form>

<%- include("partials/footer"); %>
